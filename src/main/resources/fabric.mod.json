	"authors": [
		"NickAcPT"
	],
	"contact": {
		"homepage": "https://nickac.dev/",
		"sources": "https://github.com/NickAcPT/block-corner-copy-location"
	},
	"license": "MIT",
	"icon": "assets/block-corner-copy-location/icon.png",
